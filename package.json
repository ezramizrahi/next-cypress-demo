{
  "name": "next-cypress-demo",
  "version": "0.1.0",
  "private": true,
  "author": "ezra mizrahi ezra.mizrahi@hey.com",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "postbuild": "next-on-netlify",
    "lint": "next lint",
    "cypress:open": "cypress open",
    "e2e": "cypress run --browser chrome --spec \"cypress/e2e/**/*\"",
    "cleanup": "rm -fr mochawsome-report",
    "merge-reports": "npx mochawesome-merge mochawesome-report/mochawesome.json > mochawesome-report/output.json",
    "html-report": "marge mochawesome-report/mochawesome.json",
    "report:merge": "mochawesome-merge mochawesome-report/*.json > index.json",
    "report:generate": "marge index.json --reportDir reports --assetsDir reports/assets --reportPageTitle index.html",
    "cleanup:e2e:report": "npm run cleanup && npm run e2e; npm run merge-reports && npm run html-report"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "@heroicons/react": "^1.0.6",
    "@prisma/client": "^4.6.1",
    "@supabase/supabase-js": "^2.1.0",
    "@tailwindcss/forms": "^0.5.3",
    "axios": "^1.1.3",
    "base64-arraybuffer": "^1.0.2",
    "classnames": "^2.3.2",
    "formik": "^2.2.9",
    "nanoid": "^4.0.0",
    "next": "12.2.5",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "autoprefixer": "^10.4.13",
    "cypress": "^11.0.1",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.0",
    "mocha": "^10.1.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "next-on-netlify": "^3.0.1",
    "postcss": "^8.4.19",
    "prisma": "^4.6.1",
    "tailwindcss": "^3.2.4"
  }
}
